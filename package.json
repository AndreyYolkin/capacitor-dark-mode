{
  "name": "@aparajita/capacitor-dark-mode",
  "version": "1.1.0",
  "description": "Universal dark mode support for Ionic web and native apps",
  "author": "Aparajita Fishman",
  "license": "MIT",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "engines": {
    "node": ">=16.15.1"
  },
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Plugin/",
    "*.podspec",
    "LICENSE"
  ],
  "scripts": {
    "extract-info": "node scripts/extractPackageInfo.js",
    "lint.eslint": "eslint --cache --ext .js,.cjs,.mjs,.ts --max-warnings 0",
    "lint.prettier": "prettier --cache --list-different",
    "lint.tsc": "tsc --noEmit",
    "prelint": "pnpm -s extract-info",
    "prelint.fix": "pnpm -s extract-info",
    "lint": "pnpm -s lint.eslint . && pnpm -s lint.prettier . && pnpm -s lint.tsc",
    "lint.fix": "pnpm -s lint.eslint --fix . && pnpm -s lint.prettier --write . && pnpm -s lint.tsc",
    "tsc": "tsc ${SOURCE_MAP}",
    "builder": "rimraf dist && pnpm -s tsc && rollup -c rollup.config.mjs && pnpm -s docgen",
    "build": "pnpm -s builder",
    "build.dev": "SOURCE_MAP=--sourceMap pnpm -s builder",
    "docgen": "docgen --api DarkModePlugin --output-readme README.md && docgen-format",
    "open.ios": "open ios/Plugin.xcworkspace",
    "open.android": "open -b com.google.android.studio android",
    "verify": "pnpm verify:ios && pnpm verify:android && pnpm verify:web",
    "verify.ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin -quiet && cd ..",
    "verify.android": "cd android && ./gradlew clean build test && cd ..",
    "watch": "nodemon --exec 'pnpm -s build.dev'",
    "prepare": "husky install",
    "prerelease": "scripts/ensure-clean.sh && pnpm -s lint.fix",
    "release": "commit-and-tag-version",
    "release.publish": "pnpm -s release && git push --follow-tags && pnpm publish"
  },
  "commit-and-tag-version": {
    "scripts": {
      "postbump": "pnpm -s builder"
    }
  },
  "ultra": {
    "concurrent": [
      "lint",
      "verify"
    ]
  },
  "keywords": [
    "capacitor",
    "plugin",
    "native",
    "dark",
    "dark-mode"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "swiftlint": "@ionic/swiftlint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/aparajita/capacitor-dark-mode.git"
  },
  "bugs": {
    "url": "https://github.com/aparajita/capacitor-dark-mode/issues"
  },
  "devDependencies": {
    "@aparajita/capacitor-docgen": "github:aparajita/capacitor-docgen",
    "@aparajita/capacitor-docgen-format": "github:aparajita/capacitor-docgen-format",
    "@aparajita/eslint-config-base": "^1.1.5",
    "@aparajita/prettier-config": "^1.0.0",
    "@aparajita/swiftly": "^1.0.4",
    "@capacitor/cli": "^4.0.1",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@ionic/swiftlint-config": "^1.1.2",
    "@types/node": "^18.6.0",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "commit-and-tag-version": "^10.0.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "prettier-plugin-java": "^1.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.0",
    "swiftlint": "^1.0.1",
    "typescript": "~4.7.4"
    "ultra-runner": "^3.10.5"
  },
  "dependencies": {
    "@aparajita/capacitor-native-decorator": "^2.0.8",
    "@capacitor/android": "^3.6.0",
    "@capacitor/ios": "^3.6.0",
    "@capacitor/status-bar": "^1.0.8"
  },
  "peerDependencies": {
    "@aparajita/capacitor-native-decorator": "^3.0.0",
    "@capacitor/android": "^4.0.1",
    "@capacitor/core": "^4.0.1",
    "@capacitor/ios": "^4.0.1",
    "@capacitor/status-bar": "^4.0.1",
    "tslib": "^2.4.0"
  }
}
